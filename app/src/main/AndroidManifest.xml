<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alisa.myfirstapplication">

    <uses-sdk android:minSdkVersion="5" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalculatorDisplayMessageActivity" />
        <activity
            android:name=".ConstraintLayoutA"
            android:label="@string/title_activity_constraint_layout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CalculatorActivity" />
        <activity android:name=".ConstraintLayoutB" />
        <activity android:name=".ConstraintLayoutC" />
        <activity android:name=".BirthdayActivity" />
        <activity android:name=".ActivitySetFields" />
        <activity android:name=".PreferenceActivity" />
        <activity android:name=".SettingsActivity"></activity>
    </application>

</manifest>